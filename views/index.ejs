<body>
    <h1>Mini Messageboard</h1>
    <div id="messages-container">
        <% messages.forEach((message)=> { %>
            <div class="message-item">
                <p>
                    <%= message.text %>
                </p>
                <p>
                    <%= message.user %>
                </p>
                <p>
                    <%= message.added %>
                </p>
            </div>
            <% }) %>
    </div>
</body>